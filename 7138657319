--// VARIABLES \\--

local player = game:GetService("Players").LocalPlayer

getgenv().MagDistance = 0 ---@diagnostic disable-line
getgenv().JPHeight = 50 ---@diagnostic disable-line

--// LIBRARY \\--

local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Stebulous/solaris-ui-lib/main/source.lua"))()

local New = SolarisLib:New({
  Name = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,
  FolderToSave = "Eclipse"
})

local MagTab = New:Tab("Catching")
local Magging = MagTab:Section("Mags")

local PhysicsTab = New:Tab("Physics")
local JPSection = PhysicsTab:Section("JP Height")

Magging:Slider("Mag Distance", 0, 50, 0, 0.5, "1", function(value)
    getgenv().MagDistance = value ---@diagnostic disable-line
    -- console.print('Mag Distance: ' .. value)
end)

JPSection:Button("JP", function()
    getgenv().jumppower = 171.5 ---@diagnostic disable-line
-- max: 180
local get=function(x)return game:GetService(x)end local players=get('Players')local ws=get('Workspace')local rs=get('ReplicatedStorage')local rus=get('RunService')local uis=get('UserInputService')local coregui=get('CoreGui')local http=get('HttpService')local ts=get('TeleportService')local lighting=get('Lighting')local plr=players.LocalPlayer local char=(plr and (plr.Character or plr.CharacterAdded:Wait())or nil)local hrp=plr and (char:WaitForChild('HumanoidRootPart'))or nil local hum=plr and (char:WaitForChild('Humanoid'))or nil if plr then plr.CharacterAdded:Connect(function(c)char=c hrp=c:WaitForChild'HumanoidRootPart'hum=c:WaitForChild('Humanoid')end)end local cf=CFrame local v3=Vector3 local ud2=UDim2 local c3=Color3 local rgb=c3.fromRGB local step=rus.Stepped local rstep=rus.RenderStepped local heartbeat=rus.Heartbeat local JSON={stringify=function(...)return http:JSONEncode(...)end,parse=function(...)return http:JSONDecode(...)end}
local bf = Instance.new("BodyForce",hrp)
bf.Name = 'FFmover'
spawn(function()
    while step:wait() do
        local JP = getgenv().jumppower/100*10 ---@diagnostic disable-line
        if not bf then
            bf = Instance.new("BodyForce",hrp)
            bf.Name = 'FFmover'
        end
        bf.Force = v3.new(0, JP*80, 0)
    end
end)
end)

--// MAIN \\--

function dist(p1,p2)
    local pos1 = typeof(p1) == 'Instance' and p1.Position or p1
    local pos2 = typeof(p2) == 'Instance' and p2.Position or p2
    return (pos1-pos2).Magnitude
  end

spawn(function()
    while task.wait() do
        if workspace.Balls_Kickz:FindFirstChild("Football") then
            local ball = workspace.Balls_Kickz.Football
            if (ball.Position - player.Character.HumanoidRootPart.Position).Magnitude < getgenv().MagDistance then ---@diagnostic disable-line
                    firetouchinterest(player.Character["Right Arm"], ball, 0) ---@diagnostic disable-line
                    firetouchinterest(player.Character["Left Arm"], ball, 0) ---@diagnostic disable-line
                    task.wait(0.1)
                    firetouchinterest(player.Character["Right Arm"], ball, 1) ---@diagnostic disable-line
                    firetouchinterest(player.Character["Left Arm"], ball, 1) ---@diagnostic disable-line
            end
        end
    end
end)
